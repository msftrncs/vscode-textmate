    "durations": "^3.4.1",
    "istanbul": "^0.4.4",
    "mocha": "^5.2.0",
    "onigasm": "^2.2.1",
    "remap-istanbul": "^0.6.4",
    "tslint": "^3.15.1",
    "typescript": "^2.9.2",
    "typings": "^1.3.2"
