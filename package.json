{
  "name": "vscode-textmate",
  "version": "4.0.1",
  "description": "VSCode TextMate grammar helpers",
  "author": {
    "name": "Microsoft Corporation"
  },
  "main": "./release/main.js",
  "typings": "./release/main.d.ts",
  "repository": {
    "type": "git",
